{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "PlateRecognition": {
    "Enabled": true,
    "Provider": "viettel",
    "BaseUrl": "https://viettelai.vn/plate/detect_plate",
    "RecognizeEndpoint": "",
    "ApiKey": "YOUR_VIETTEL_API_KEY_HERE",
    "TimeoutSeconds": 30
  },
  "Jwt": {
    "Key": "YOUR_DEVELOPMENT_SECRET_KEY_MUST_BE_AT_LEAST_32_CHARS",
    "Issuer": "ParkingSystem",
    "Audience": "ParkingFrontend"
  }
}